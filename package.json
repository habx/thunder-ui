{
  "name": "@habx/lib-client-backoffx",
  "version": "1.26.0",
  "description": "UI Library for client-crm-v2 and client-housings",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project tsconfig.json 'src/**/*.ts' &&  eslint ./src/",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "compile": "rimraf ./lib && NODE_ENV=production npm run build:js && npm run build:types",
    "watch": "NODE_ENV=development npm run build:js:watch",
    "build:js": "NODE_ENV=production ./node_modules/.bin/babel src --out-dir lib --extensions '.es6,.js,.es,.jsx,.mjs,.ts,.tsx'",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js:watch": "npm run build:js -- --watch",
    "build:types:watch": "npm run build:types -- --watch",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./assets",
    "build-storybook": "build-storybook -c .storybook -s ./assets -o .storybook-static",
    "install:local": "npm ci && npm link styled-components && npm link react && npm link react-dom"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/lib-client-backoffx.git"
  },
  "author": "Habx Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/lib-client-backoffx/issues"
  },
  "homepage": "https://github.com/habx/lib-client-backoffx#readme",
  "dependencies": {
    "clean-tag": "^2.0.2",
    "color": "^3.1.0",
    "rc-slider": "^8.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-centered": "^4.0.7",
    "@storybook/addon-info": "^4.1.2",
    "@storybook/addon-knobs": "^4.0.7",
    "@storybook/addon-storyshots": "^4.1.2",
    "@storybook/react": "^4.0.7",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.118",
    "@types/react": "16.7.0",
    "@types/react-dom": "^16.0.11",
    "@types/storybook__react": "^4.0.0",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-habx": "^1.1.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "16.3.0 - 16.7.X",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "16.3.0 - 16.7.X",
    "react-test-renderer": "^16.6.3",
    "recompose": "^0.30.0",
    "rimraf": "^2.6.2",
    "styled-components": "^4.1.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/assets"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
