{
  "name": "@habx/thunder-ui",
  "version": "4.4.0",
  "description": "Flawless Prototyping React library",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && bili && npm run build:types",
    "build:dev": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "postbuild": "node ./scripts/postbuild.js",
    "publish": "cd dist && npm publish && cd ..",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:dark": "cross-env STORYBOOK_THEME=dark npm run storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook-static",
    "storybook:deploy": "storybook-to-ghpages -s storybook:build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/thunder-ui.git"
  },
  "author": "Habx Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/thunder-ui/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "homepage": "https://github.com/habx/thunder-ui#readme",
  "dependencies": {
    "@testing-library/react": "^8.0.1",
    "clean-tag": "3.1.0",
    "color": "^3.1.0",
    "lodash.deburr": "^4.1.0",
    "lodash.get": "^4.4.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styled-components": ">=4.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-centered": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-storysource": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/sinon": "^7.0.11",
    "@types/storybook__react": "^4.0.1",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "bili": "^4.7.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-import-helpers": "^0.1.4",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.1.4",
    "lint-staged": "^8.1.6",
    "lodash": "^4.17.0",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "recompose": "^0.30.0",
    "rimraf": "^2.6.3",
    "rollup-plugin-typescript2": "^0.21.0",
    "sinon": "^7.3.2",
    "styled-normalize": "^8.0.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
